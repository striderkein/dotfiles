{
  "breadcrumbs.enabled": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.zoomLevel": -3,
  "editor.minimap.enabled": false,
  "workbench.startupEditor": "newUntitledFile",
  "vscodeWorkspaceSwitcher.paths": [
    "~/code-workspace"
  ],
  "files.autoSave": "off",
  "files.hotExit": "onExitAndWindowClose",
  "files.autoGuessEncoding": true,
  "terminal.integrated.fontFamily": "'HackGen Console', 'SFMono-Square', menlo, monospace",
  "editor.fontFamily": "'HackGen Console', 'SFMono-Square', Menlo, Monaco, 'Courier New', monospaceth",
  "editor.insertSpaces": true,
  "editor.renderWhitespace": "all",
  "editor.renderControlCharacters": true,
  "editor.tabSize": 2,
  "terminal.external.osxExec": "iTerm.app",
  "vim.hlsearch": true,
  "vim.ignorecase": true,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.variables": {
    "lang": "ja"
  },
  "explorer.confirmDelete": false,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.rendererType": "dom",
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.iconTheme": "material-icon-theme",
  "editor.largeFileOptimizations": false,
  "vim.useSystemClipboard": false,
  "[yaml]": {},
  "explorer.confirmDragAndDrop": false,
  "vim.overrideCopy": false,
  "files.exclude": {
    "**/.git": false
  },
  "intelephense.files.exclude": [
    "**/.hg/**",
    "**/CVS/**",
    "**/.DS_Store/**",
    "**/node_modules/**",
    "**/bower_components/**",
    "**/vendor/**/{Tests,tests}/**",
    "**/.svn/**"
  ],
  "hediet.vscode-drawio.local-storage": "",
  "workbench.colorTheme": "Dracula Soft",
  "eslint.workingDirectories": [
    "./client",
    "./server"
  ],
  "code-eol.crlfCharacter": "‚Üê",
  "files.eol": "\n",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "diffEditor.renderSideBySide": true,
  "workbench.editor.empty.hint": "hidden",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "terminal.integrated.tabStopWidth": 4,
  "editor.fontSize": 14,
  "terminal.integrated.fontSize": 14,
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "debug.console.fontSize": 13,
  "explorer.confirmPasteNative": false,
  "editor.formatOnPaste": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "prettier.printWidth": 120,
  "java.saveActions.organizeImports": false,
  "java.jdt.ls.java.home": "",
  "vim.camelCaseMotion.enable": true,
  "git.autofetch": true,
  "gitlens.ai.experimental.provider": "openai",
  "gitlens.ai.experimental.openai.model": "gpt-4o",
  "github.copilot.editor.enableAutoCompletions": true,
  "vscode-pets.throwBallWithMouse": true,
  "vscode-pets.petSize": "small",
  "eslint.validate": [],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "chat.agent.enabled": true,
  "remote.defaultExtensionsIfInstalledLocally": [
    "GitHub.copilot",
    "GitHub.copilot-chat",
    "GitHub.vscode-pull-request-github"
  ],
  "github.copilot.nextEditSuggestions.enabled": true
}
